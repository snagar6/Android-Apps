<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.sample.android.teeserviceclient">

 <uses-permission android:name="com.sample.android.teeservice.TEE_REQUEST" />
 
 <application android:label="@string/app_name">
    <uses-library android:name="com.sample.android.service.tee"
      android:required="true" />
    <activity android:name=".TeeActivity"
      android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

</manifest>
